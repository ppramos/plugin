{
    "manifest_version": 2,
    "version": "0.0.1",
    "name": "CVS Output",
    "description": "CVS Output",
    "author": "Pedro Ramos",
    "browser_action": {
		"default_title": "CVS Output",
		"default_popup": "index.html"
    },
	"web_accessible_resources": [
		"css/bootstrap.min.css"
	],
	"content_scripts": [{
		"matches": ["<all_urls>"],
		"js": ["js/jquery-3.4.1.min.js", "js/bootstrap.min.js"]
	}],
	"permissions": [
		"activeTab",
		"http://*/",
		"storage"
	]
}